<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cafeyvinowinebar.Cafe_y_Vino">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/logo_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/logo_icon"
        android:theme="@style/Theme.Double_test">
        <activity
            android:name=".PrivacyActivity"
            android:exported="false" />
        <activity
            android:name=".MenuProductActivity"
            android:exported="false" />
        <activity android:name=".UserDataActivity" />
        <activity android:name=".WalkthroughActivity" />
        <activity android:name=".ReservaMesaActivity" />
        <activity android:name=".VinosMenuActivity" />
        <activity android:name=".GenericCategoryActivity" />
        <activity android:name=".MainMenuActivity" />
        <activity android:name=".ReservaFechaActivity" />
        <activity android:name=".CuentaActivity" />
        <activity android:name=".PlanActivity" />
        <activity android:name=".ReservaFinActivity" />
        <activity android:name=".GiftShopActivity" />
        <activity android:name=".CanastaActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".RegistrationActivity" />
        <activity android:name=".BienvenidoActivity" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service
            android:name=".MyFirebaseMessagingService"
            android:exported="false">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
            <intent-filter>
                <action android:name="com.google.firebase.INSTANCE_ID_EVENT" />
            </intent-filter>
        </service>

        <meta-data
            android:name="com.google.firebase.messaging.default_notification_icon"
            android:resource="@drawable/logo_mini" />
        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>